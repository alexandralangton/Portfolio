{
  "name": "alexandra_langton_portfolio",
  "version": "1.0.0",
  "description": "A virtual resume of my projects and roles",
  "main": "public/index.html",
  "scripts": {
    "start": "webpack-dev-server --open",
    "start-dev": "webpack -w & webpack serve",
    "build": "webpack --mode production",
    "build-dev": "webpack",
    "deploy": "aws s3 sync public s3://alangtonportfolio --exclude bundle.js.map",
    "deploy-single": "aws s3 cp public/index.html s3://alangtonportfolio",
    "deploy-bundle": "aws s3 cp public/bundle.js.gz s3://alangtonportfolio --content-type 'text/javascript' --content-encoding 'gzip'"
  },
  "author": "Alexandra Langton",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "babel-loader": "^8.2.2",
    "compression-webpack-plugin": "^7.1.2",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.1",
    "file-loader": "^6.2.0",
    "imagemin-webp": "^6.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "style-loader": "^2.0.0",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.13",
    "aos": "^3.0.0-beta.6",
    "path": "^0.12.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scroll": "^1.8.1"
  }
}
